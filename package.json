{
  "name": "jcg-exam-builder-backend",
  "version": "1.0.0",
  "description": "VBG exam builder",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec npx ts-node server.ts",
    "build-dev": "node ./shell/build.mjs dev",
    "build-staging": "node ./shell/build.mjs staging",
    "build-prod": "node ./shell/build.mjs prod",
    "lint": "eslint --ext .ts",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "prettier": "prettier --check **/*.ts",
    "prettier-fix": "npm run prettier -- --write **/*.ts",
    "test": "npx jest --rootDir=__tests__",
    "watch-test": "npx jest --rootDir=__tests__ --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaenterprises/jcg-exam-builder-backend.git"
  },
  "author": "Cal Vert Wong @ Veteran Benefits Guide",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vaenterprises/jcg-exam-builder-backend.git/issues"
  },
  "homepage": "https://github.com/vaenterprises/jcg-exam-builder-backend.git#readme",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "@types/shelljs": "^0.8.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "shelljs": "^0.8.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "hpp": "^0.2.3",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf.js-extract": "^0.2.1",
    "short-unique-id": "^4.4.4",
    "uuidv4": "^6.2.13"
  }
}
